{
  "name": "AntiNFT.Extension",
  "version": "0.3.2",
  "manifest_version": 3,
  "description": "This extension blocks NFT related websites/Marketplaces, Pro-NFT social media pages, and Pro-NFT discord server invites.",
  "declarative_net_request" : {
    "rule_resources" : [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "content.json"
    }]
  },
  "action": {
    "default_title": "AntiNFT Extension",
    "default_popup": "small_win.html",
    "default_icon": {
      "32": "icon_32.png"
    }
  },
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "background": {
    "service_worker": "background.js"
  }
}